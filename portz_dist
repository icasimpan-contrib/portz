PNAME=`basename $0`
set -e
portz_root=`dirname $0`
portz_root=$PWD

. ${portz_root}/portz_common.sh

inform "prefix      = $prefix"
inform "exec_prefix = $exec_prefix"

packages=$*

for package in $packages ; do
    portz_dist $package
done        
    
